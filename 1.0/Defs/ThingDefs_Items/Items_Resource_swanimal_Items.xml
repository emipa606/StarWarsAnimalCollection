    </graphicData>
    <stuffProps>
      <color>(69,39,32)</color>
      <statFactors>
        <ArmorRating_Sharp>1.1</ArmorRating_Sharp>
        <ArmorRating_Heat>2.2</ArmorRating_Heat>
