  <!-- ====== TROPHIES ====== -->
  <ThingDef ParentName="ResourceVerbBase">
    <defName>BanthaHorn</defName>
    <label>bantha horn</label>
    <description>A bantha's horn. Good enough to fend off some enemies. Comes as at least a pair, so not as valuable as other horns out there.</description>
    <graphicData>
      <texPath>swanimals/BanthaHorn</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <equippedAngleOffset>-20</equippedAngleOffset>
    <statBases>
      <MarketValue>150</MarketValue>
      <MaxHitPoints>100</MaxHitPoints>
      <Mass>4</Mass>
      <Flammability>.4</Flammability>
      <DeteriorationRate>0.3</DeteriorationRate>
    </statBases>
    <resourceReadoutPriority>Middle</resourceReadoutPriority>
    <stackLimit>10</stackLimit>
    <tools>
      <li>
        <label>point</label>
        <capacities>
          <li>Scratch</li>
          <li>Stab</li>
        </capacities>
        <power>7</power>
        <cooldownTime>1.85</cooldownTime>
      </li>
      <li>
        <label>base</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>5</power>
        <cooldownTime>1.85</cooldownTime>
      </li>
    </tools>
    <thingCategories>
      <li>Items</li>
    </thingCategories>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
    <thingSetMakerTags>
      <li>AnimalPart</li>
    </thingSetMakerTags>
  </ThingDef>
