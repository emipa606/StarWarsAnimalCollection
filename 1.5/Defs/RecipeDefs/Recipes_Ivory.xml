<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef MayRequire="LegendaryMinuteman.SimpleIvory">
    <defName>ProcessIvoryBantha</defName>
    <label>process ivory from raw bantha horn</label>
    <description>Produces beautiful ivory from raw bantha horn.</description>
    <jobString>Processing bantha horn into ivory.</jobString>
    <workAmount>1200</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>CutStone</effectWorking>
    <soundWorking>Recipe_MakeStoneBlocks</soundWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>BanthaHorn</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <Ivory>15</Ivory>
    </products>
    <recipeUsers>
      <li>TableStonecutter</li>
    </recipeUsers>
  </RecipeDef>
